@model VendorManagement.Models.ViewModels.AddAuditCycleViewModel

@{
}
<h1>Add Audit Cycle</h1>
<hr />

<form asp-action="AddAuditCycle" method="post">
    <div class="form-group mb-3">
        <label asp-for="AuditCycleId" class="control-label"></label>
        <input asp-for="AuditCycleId" class="form-control" readonly />
        <span asp-validation-for="AuditCycleId" class="text-danger"></span>
    </div>
    <div class="form-group mb-3">
        <label asp-for="AuditCycleType" class="control-label"></label>
        <input asp-for="AuditCycleType" class="form-control" />
        <span asp-validation-for="AuditCycleType" class="text-danger"></span>
    </div>
    <div class="form-group mb-3">
        <input type="submit" value="Add Audit Cycle" class="btn btn-primary" />
    </div>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
